<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="RegressionSuite" configfailurepolicy="continue" parallel="tests" thread-count="6">
    <test name="Calculator Test">
        <classes>
            <class name="org.internship.CalculatorTest">
                <methods>
                    <include name="testCalculator"/>
                    <include name="testCalculatorSoftAssert"/>
                    <include name="testCalculatorWithPrivate"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Login Test">
        <classes>
            <class name="org.internship.LoginTest">
                <methods>
                    <include name="testLoginUsername"/>
                    <include name="testLoginPassword"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>